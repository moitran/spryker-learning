<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>
    <transfer name="CustomerProductPriceCollection">
        <property name="customerProducts" type="CustomerProduct[]" singular="customerProduct"/>
    </transfer>

    <transfer name="CustomerProduct">
        <property name="customerNumber" type="string"/>
        <property name="productNumber" type="string"/>
        <property name="customerProductPrices" type="CustomerProductPrice[]" singular="customerProductPrice"/>
    </transfer>

    <transfer name="CustomerProductPrice">
        <property name="quantity" type="int"/>
        <property name="price" type="int"/>
    </transfer>
</transfers>
