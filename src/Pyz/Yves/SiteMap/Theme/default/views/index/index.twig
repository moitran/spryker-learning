{% autoescape %}
<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in 1..totalPage %}
        <sitemap>
            <loc>{{ url('site-map-detail', {pageNumber: page}) }}</loc>
        </sitemap>
    {% endfor %}
</sitemapindex>
{% endautoescape %}
